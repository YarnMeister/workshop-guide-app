# OpenRouter API Key (for AI enhancement features)
VITE_OPEN_ROUTER_API_KEY=sk-or-v1-your-key-here

# Participant data (JSON object mapping codes to participant info)
# Format: {"code1":{"name":"Name1","apiKey":"sk-or-v1-..."},"code2":{"name":"Name2","apiKey":"sk-or-v1-..."}}
PARTICIPANTS_JSON={}

# Cookie secret for session management (generate a random string)
COOKIE_SECRET=your-secret-key-here

# Neon Database URL (PostgreSQL connection string)
DATABASE_URL=postgresql://user:password@host/database?sslmode=require

